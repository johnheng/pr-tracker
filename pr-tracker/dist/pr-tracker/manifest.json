{
    "manifest_version": 2,
    "name": "GitHub PR Tracker",
    "version": "1.0.0",
    "permissions": [
    ],
    "background": {
        "page": "index.html#/homepage",
        "persistent": true
    },
    "browser_action": {
        "default_title": "GitHub PR Tracker",
        "default_popup": "index.html#/homepage"
    },
    "icons": {
        "19": "assets/icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}